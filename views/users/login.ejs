<% layout('layouts/boilerplate') -%>
<div class="row mt-4">
  <div class="col-6 offset-3">
    <h1>Login</h1>
    <form method="POST" novalidate action="/login" class="needs-validation">
      <div class="mb-3">
        <label for="username" class="form-label">Username</label>
        <input
          type="text"
          name="username"
          id="username"
          class="form-control"
          required
        />
      </div>

    
      <div class="mb-3">
        <label for="password" class="form-label">Password</label>
        <input
          type="text"
          name="password"
          id="password"
          class="form-control"
          required
        />
      </div>
      <button class="btn btn-outline-success">LogIn</button>

    </form>
  </div>
</div>
